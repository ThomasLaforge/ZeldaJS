{
  "name": "zelda-html5",
  "version": "0.3.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bobbylight/ZeldaJS.git"
  },
  "scripts": {
    "clean": "rimraf build doc",
    "build": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.js",
    "watch": "cross-env NODE_ENV=dev webpack --config webpack/webpack.config.js --watch",
    "dev": "cross-env NODE_ENV=dev webpack-dev-server --config ./webpack/webpack.config.js",
    "webpack-analyze": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.js --json > size-info.json",
    "webpack-size-view": "webpack-bundle-size-analyzer size-info.json > size-info.txt",
    "start": "electron src/js/electron-main.js",
    "test": "NODE_ENV=test karma start",
    "doc": "typedoc --mode file --target ES5 --out doc/ ."
  },
  "devDependencies": {
    "@types/jquery": "3.2.12",
    "@types/react": "15.0.29",
    "@types/react-bootstrap": "0.0.50",
    "@types/react-dom": "15.5.0",
    "@types/react-redux": "4.4.44",
    "@types/redux": "3.6.0",
    "@types/redux-actions": "1.2.6",
    "copy-webpack-plugin": "4.0.1",
    "cross-env": "5.0.5",
    "css-loader": "0.28.5",
    "electron": "1.7.5",
    "less": "2.7.2",
    "less-loader": "4.0.5",
    "rimraf": "2.6.1",
    "string-replace-webpack-plugin": "0.1.3",
    "style-loader": "0.18.2",
    "ts-loader": "2.3.3",
    "tslint": "5.7.0",
    "tslint-loader": "3.5.3",
    "typedoc": "0.8.0",
    "typescript": "2.4.2",
    "url-loader": "0.5.9",
    "webpack": "2.4.1",
    "webpack-bundle-size-analyzer": "2.7.0",
    "webpack-dev-server": "2.4.5",
    "webpack-target-electron-renderer": "0.4.0"
  },
  "dependencies": {
    "bootstrap": "3.3.7",
    "font-awesome": "4.7.0",
    "gtp": "1.2.1",
    "jquery": "3.2.1",
    "jshighlight": "0.2.1",
    "react": "15.6.1",
    "react-bootstrap": "0.31.0",
    "react-dom": "15.6.1",
    "react-redux": "5.0.5",
    "redux": "3.7.0",
    "redux-actions": "2.0.3"
  }
}
