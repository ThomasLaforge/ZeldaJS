{
  "name": "zelda-html5",
  "version": "0.3.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bobbylight/ZeldaJS.git"
  },
  "scripts": {
    "clean": "rimraf build doc",
    "build": "cross-env NODE_ENV=production webpack -p --config webpack/webpack.config.js",
    "watch": "cross-env NODE_ENV=dev webpack --config webpack/webpack.config.js --watch",
    "dev": "cross-env NODE_ENV=dev webpack-dev-server --config ./webpack/webpack.config.js",
    "webpack-analyze": "cross-env NODE_ENV=production webpack -p --config webpack/webpack.config.js --json > size-info.json",
    "webpack-size-view": "webpack-bundle-size-analyzer size-info.json > size-info.txt",
    "start": "electron src/js/electron-main.js",
    "test": "cross-env NODE_ENV=test karma start",
    "doc": "typedoc --mode file --target ES5 --out doc/ ./src/app"
  },
  "devDependencies": {
    "@types/jasmine": "2.8.3",
    "@types/jquery": "3.2.17",
    "@types/react": "16.0.34",
    "@types/react-bootstrap": "0.31.9",
    "@types/react-dom": "16.0.3",
    "@types/react-redux": "5.0.14",
    "@types/redux-actions": "2.2.3",
    "copy-webpack-plugin": "4.3.1",
    "cross-env": "5.1.3",
    "css-loader": "0.28.8",
    "electron": "1.7.10",
    "istanbul-instrumenter-loader": "3.0.0",
    "jasmine": "2.8.0",
    "karma": "2.0.0",
    "karma-coverage": "1.1.1",
    "karma-jasmine": "1.1.1",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-sourcemap-loader": "0.3.7",
    "karma-spec-reporter": "0.0.32",
    "karma-typescript-preprocessor": "0.3.1",
    "karma-webpack": "2.0.9",
    "less": "2.7.3",
    "less-loader": "4.0.5",
    "phantomjs": "2.1.7",
    "phantomjs-polyfill": "0.0.2",
    "rimraf": "2.6.2",
    "string-replace-webpack-plugin": "0.1.3",
    "style-loader": "0.19.1",
    "ts-loader": "3.2.0",
    "tslint": "5.8.0",
    "tslint-loader": "3.5.3",
    "typedoc": "0.9.0",
    "typescript": "2.6.2",
    "url-loader": "0.6.2",
    "webpack": "3.10.0",
    "webpack-bundle-size-analyzer": "2.7.0",
    "webpack-dev-server": "2.10.0",
    "webpack-target-electron-renderer": "0.4.0"
  },
  "dependencies": {
    "bootstrap": "3.3.7",
    "font-awesome": "4.7.0",
    "gtp": "1.3.1",
    "jquery": "3.2.1",
    "jshighlight": "0.2.1",
    "react": "16.2.0",
    "react-bootstrap": "0.32.0",
    "react-dom": "16.2.0",
    "react-redux": "5.0.6",
    "redux": "3.7.2",
    "redux-actions": "2.2.1"
  }
}
