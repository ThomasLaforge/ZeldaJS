  },
  "scripts": {
    "clean": "rimraf build doc",
    "build": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.js",
    "watch": "cross-env NODE_ENV=dev webpack --config webpack/webpack.config.js --watch",
    "dev": "cross-env NODE_ENV=dev webpack-dev-server --config ./webpack/webpack.config.js",
    "build-stats": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.js --json > stats.json",
    "start": "electron src/js/electron-main.js",
    "test": "NODE_ENV=test karma start",
    "doc": "typedoc --mode file --target ES5 --out doc/ ."
  },
  "devDependencies": {
    "@types/angular": "1.6.19",
    "@types/angular-ui-bootstrap": "0.13.43",
