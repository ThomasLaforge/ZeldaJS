    "copy-webpack-plugin": "4.0.1",
    "cross-env": "5.0.1",
    "css-loader": "0.28.4",
    "electron": "1.6.11",
    "rimraf": "2.6.1",
    "string-replace-webpack-plugin": "0.1.3",
    "style-loader": "0.18.2",
    "ts-loader": "2.1.0",
    "tslint": "5.4.3",
